  <inertial>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <mass value="0.639"/>
    <inertia ixx="3.02e-3" ixy="0" ixz="0" iyy="8.64e-4" iyz="0" izz="3.39e-3"/>
  </inertial>
</link>


